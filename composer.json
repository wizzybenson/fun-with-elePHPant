{
    "name": "fun-with-elePHPant",
    "type": "standard",
    "description": "Algorithms and Datastructures using PHP",
    "homepage": "https://github.com/wizzybenson/fun-with-elePHPant",
    "license": "CC-BY-SA-4.0",
    "autoload": {
        "psr-4": {
            "FunwithelePHPant\\Datastructures\\": "src/Datastructures",
            "FunwithelePHPant\\Algorithms\\": "src/Algorithms",
            "FunwithelePHPant\\Exceptions\\": "src/Exceptions",
            "FunwithelePHPant\\Benchmark\\Framework\\": "lib/benchmark/",
            "FunwithelePHPant\\Config\\": "config/"
        },
        "files": ["src/functions/fn.fundamentals.php"] 
    },
    "autoload-dev": {
        "psr-4": {
            "FunwithelePHPant\\Datastructures\\": "tests/Datastructures",
            "FunwithelePHPant\\Algorithms\\": "tests/Algorithms"
        }
    },
    "bin": ["bin/fun-with-elePHPant"],
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    }
}
